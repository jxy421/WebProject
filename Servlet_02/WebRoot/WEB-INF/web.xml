<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	<servlet>
		<servlet-name>FormServlet</servlet-name>
		<servlet-class>web.FormServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>FormServlet</servlet-name>
		<url-pattern>/sayHi</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>FormGetServlet</servlet-name>
		<servlet-class>web.FormGetServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>FormGetServlet</servlet-name>
		<url-pattern>/sayHi2</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>employServlet</servlet-name>
		<servlet-class>web.employServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>employServlet</servlet-name>
		<url-pattern>/addemp</url-pattern>
	</servlet-mapping>
</web-app>
